{
    "name": "quip-cli",
    "version": "1.0.0-alpha.16",
    "description": "A Command Line Interface for the Quip Live Apps platform",
    "main": "dist/qla",
    "files": [
        "dist"
    ],
    "bin": {
        "quip-cli": "dist/qla",
        "qla": "dist/qla"
    },
    "scripts": {
        "build": "./build.sh",
        "build:watch": "onchange src/**/*.ts -- npm run build",
        "test": "npm run build && tape test/**/*.test.js",
        "test:watch": "onchange src/**/*.ts test/**/*.js -- npm run test",
        "prepare": "npm run build"
    },
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/quip/quip-apps.git"
    },
    "keywords": [
        "quip",
        "live",
        "apps"
    ],
    "author": "Quip",
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/quip/quip-apps/issues"
    },
    "homepage": "https://github.com/quip/quip-apps#readme",
    "devDependencies": {
        "@types/inquirer": "^6.5.0",
        "@types/ncp": "^2.0.4",
        "@types/node": "^13.13.5",
        "@zeit/ncc": "^0.22.3",
        "onchange": "^7.0.2",
        "parcel": "^1.12.4",
        "tape": "^5.0.0",
        "tape-watch": "^2.3.0",
        "typescript": "^3.8.3"
    },
    "dependencies": {
        "arg": "^4.1.3",
        "create-quip-app": "0.0.45",
        "inquirer": "^7.1.0",
        "ncp": "^2.0.0"
    }
}
