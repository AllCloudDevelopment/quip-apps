{
    "id": "NKMAjAPb7KP",
    "manifest_version": 1,
    "name": "Spotify",
    "version_name": "1.0",
    "version_number": 1,
    "sizing_mode": "fit_content",
    "initial_height": 80,
    "initial_width": 300,
    "toolbar_color": "green",
    "js_files": ["dist/app.js"],
    "css_files": ["dist/app.css"],
    "csp_sources": {
        "frame_srcs": [
            "https://open.spotify.com",
            "https://embed.spotify.com"
        ]
    },
    "intercept_url_patterns": [
        "https://open.spotify.com/track/",
        "https://open.spotify.com/user/*/playlist/"
    ]
}
